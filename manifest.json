{
  "manifest_version": 3,
  "name": "TabKondo",
  "version": "1.0.1",
   "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["tabs", "scripting", "storage", "notifications"],
  "host_permissions": ["https://readwise.io/api/v3/save/*"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}


